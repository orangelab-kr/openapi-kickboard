{
  "name": "openapi-kickboard",
  "description": "Kickboard control, or you can include real-time to determine the location of Kickboard.",
  "version": "1.11.12",
  "main": "dist/src/index.js",
  "repository": "https://github.com/orangelab-kr/openapi-kickboard",
  "author": "bakjonghun <bakjonghun@hikick.kr>",
  "license": "MIT",
  "scripts": {
    "start": "doppler run --fallback=doppler.encrypted.json --command \"node .\"",
    "serve": "yarn clean && doppler run --command \"tsc-watch --onSuccess \\\"node .\\\"\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prebuild": "yarn clean",
    "build": "tsc",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "@hikick/kickboard-sdk": "^1.4.7",
    "@hikick/openapi-internal-sdk": "^1.6.13",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.1",
    "express": "^4.18.0",
    "express-ws": "^5.0.2",
    "firebase-admin": "^10.1.0",
    "i18n": "^0.14.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "nodes2ts": "^2.0.0",
    "superagent": "^7.1.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/i18n": "^0.13.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.29",
    "@types/prettier": "^2.6.0",
    "@types/superagent": "^4.1.15",
    "@types/tsc-watch": "4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "tsc-watch": "5.0.3",
    "typescript": "4.6.3"
  }
}
